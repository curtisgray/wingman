{
    "name": "wingman",
    "author": {
        "name": "Electric Curtis",
        "url": "https://x.com/electric_curtis"
    },
    "description": "Wingman is the best way to run Llama AI locally.",
    "version": "0.8.7",
    "private": true,
    "main": "tron/main.js",
    "scripts": {
        "dev": "next dev -p 2000",
        "pstart": "next start -p 2000",
        "debug": "cross-env NODE_OPTIONS='--inspect' next dev",
        "build": "next build",
        "ebuild": "npm run build && cpx \".next/static/**/*\" .next/standalone/.next/static",
        "build:wingman": "tsc --project tsconfig.wingman.json",
        "lint": "next lint",
        "format": "prettier --write .",
        "test": "vitest",
        "coverage": "vitest run --coverage",
        "pretty": "prettier --write \"./{components,hooks,pages,services,types,utils}/**/*.{js,jsx,ts,tsx,json}\"",
        "estart": "electron .",
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "npm run ebuild && electron-forge make"
    },
    "config": {
        "forge": "./forge.config.js"
    },
    "dependencies": {
        "@dqbd/tiktoken": "^1.0.13",
        "@headlessui/react": "^1.7.18",
        "@tabler/icons-react": "^2.9.0",
        "electron-squirrel-startup": "^1.0.0",
        "eventsource-parser": "^0.1.0",
        "i18next": "^22.5.1",
        "lodash": "^4.17.21",
        "next": "^13.2.4",
        "next-i18next": "^13.3.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-hot-toast": "^2.4.1",
        "react-i18next": "^12.3.1",
        "react-markdown": "^9.0.1",
        "react-query": "^3.39.3",
        "react-select": "^5.8.0",
        "react-syntax-highlighter": "^15.5.0",
        "react-tooltip": "^5.26.3",
        "react-use-websocket": "^4.7.0",
        "recharts": "^2.12.1",
        "rehype-mathjax": "^4.0.0",
        "remark-gfm": "^4.0.0",
        "remark-math": "^6.0.0",
        "sharp": "^0.33.2",
        "sqlite3": "^5.1.7",
        "systeminformation": "^5.22.0",
        "tree-kill": "^1.2.2",
        "update-electron-app": "^3.0.0",
        "use-immer": "^0.9.0",
        "uuid": "^9.0.1",
        "winston": "^3.11.0",
        "winston-daily-rotate-file": "^5.0.0"
    },
    "devDependencies": {
        "@electron-forge/cli": "^7.3.0",
        "@electron-forge/maker-deb": "^7.3.0",
        "@electron-forge/maker-dmg": "^7.3.0",
        "@electron-forge/maker-rpm": "^7.3.0",
        "@electron-forge/maker-squirrel": "^7.3.0",
        "@electron-forge/maker-zip": "^7.3.0",
        "@electron-forge/plugin-auto-unpack-natives": "^7.3.0",
        "@electron-forge/publisher-github": "^7.3.0",
        "@mozilla/readability": "^0.5.0",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/github": "^9.2.6",
        "@tailwindcss/typography": "^0.5.10",
        "@trivago/prettier-plugin-sort-imports": "^4.1.1",
        "@types/jsdom": "^21.1.6",
        "@types/lodash": "^4.14.202",
        "@types/node": "20.11.20",
        "@types/react": "18.2.58",
        "@types/react-dom": "18.2.19",
        "@types/react-syntax-highlighter": "^15.5.11",
        "@types/uuid": "^9.0.8",
        "@vitest/coverage-c8": "^0.29.7",
        "autoprefixer": "^10.4.17",
        "cpx": "^1.5.0",
        "cross-env": "^7.0.3",
        "electron": "^29.0.1",
        "endent": "^2.1.0",
        "eslint": "8.57.0",
        "eslint-config-next": "14.1.0",
        "jsdom": "^24.0.0",
        "prettier": "^3.2.5",
        "prettier-plugin-tailwindcss": "^0.2.5",
        "semantic-release": "^23.0.2",
        "tailwindcss": "^3.4.1",
        "typescript": "5.3.3",
        "vitest": "^0.29.7"
    }
}
